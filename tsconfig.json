{
    "compilerOptions": {
        "target": "ESNext",
        "module": "NodeNext",
        "moduleResolution": "nodenext",
        "outDir": "dist/",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "noImplicitAny": true,
        "isolatedModules": true,
        "noUncheckedIndexedAccess": true,
        "noEmit": true
    },
    "exclude":["dist", "node_modules", "*.{js,mjs}", "**.test.**"],
}
